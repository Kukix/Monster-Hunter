<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <wsDualHttpBinding>
                <binding name="WSDualHttpBinding_ILogin" />
                <binding name="WSDualHttpBinding_ILobby" />
                <binding name="WSDualHttpBinding_IGame" />
            </wsDualHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/MonsterFeastService/CGame/"
                binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_ILogin"
                contract="MonsterFeastService.ILogin" name="WSDualHttpBinding_ILogin">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/MonsterFeastService/CGame/"
                binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_ILobby"
                contract="MonsterFeastService.ILobby" name="WSDualHttpBinding_ILobby">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/MonsterFeastService/CGame/"
                binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_IGame"
                contract="MonsterFeastService.IGame" name="WSDualHttpBinding_IGame">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>